<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<page-template>
  <display-name>Xslt Main Page</display-name>
  <descriptor>xslt-1.0.0:xslt-landing-page</descriptor>
  <config>
    <property name="background-color" type="String">blue</property>
  </config>
  <can-render/>
  <regions>
    <region name="main">
      <part-component name="TrampolineBuy" template="bluman.trampoline|xslt-trampoline-buy">
        <config>
          <property name="text" type="String">Take a part!</property>
        </config>
      </part-component>
      <part-component name="TrampolineAccessories" template="bluman.trampoline|xslt-trampoline-accessories">
        <config>
          <property name="text" type="String">Take a part!</property>
        </config>
      </part-component>
      <layout-component name="Layout70-30" template="bluman.trampoline|xslt-trampoline-70-30">
        <regions>
          <region name="left">
            <part-component name="TrampolineDescription" template="bluman.trampoline|xslt-trampoline-description">
              <config/>
            </part-component>
          </region>
        </regions>
      </layout-component>
    </region>
  </regions>
</page-template>
