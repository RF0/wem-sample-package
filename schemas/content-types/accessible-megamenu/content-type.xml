<?xml version="1.0" encoding="UTF-8"?>
<type>
    <display-name>Accessible megamenu</display-name>
    <content-display-name-script>$('title')</content-display-name-script>
    <super-type>structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <is-built-in>false</is-built-in>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Menu title">
            <label>Menu title</label>
            <items>
                <input name="title" type="TextLine">
                    <label>Title</label>
                    <immutable>false</immutable>
                    <indexed>false</indexed>
                    <occurrences minimum="1" maximum="1"/>
                    <help-text>Menu title</help-text>
                </input>
            </items>
        </field-set>
    </form>
</type>
