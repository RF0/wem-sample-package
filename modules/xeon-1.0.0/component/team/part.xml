<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<part-component>
  <display-name>Team</display-name>
  <config>
    <field-set name="slides"> 
      <label>Members</label>
      <items>
        <form-item-set name="member">
          <label>Member</label>
          <immutable>false</immutable>
          <custom-text/>
          <help-text/>
          <occurrences minimum="0" maximum="0"/>
          <items>
            <input type="ImageSelector" name="image">
              <label>Image</label>
              <immutable>false</immutable>
              <indexed>false</indexed>
              <custom-text/>
              <help-text/>
              <occurrences minimum="0" maximum="1"/>
              <config>
                <relationship-type>image</relationship-type>
              </config>
            </input>
            <input type="TextLine" name="name">
              <label>Name</label>
              <immutable>false</immutable>
              <indexed>false</indexed>
              <custom-text/>
              <help-text/>
              <occurrences minimum="1" maximum="1"/>
            </input>
            <input type="TextLine" name="position">
              <label>Position</label>
              <immutable>false</immutable>
              <indexed>true</indexed>
              <custom-text/>
              <help-text/>
              <occurrences minimum="1" maximum="1"/>
            </input>
          </items>
        </form-item-set>
      </items>
    </field-set>
  </config>
</part-component>
